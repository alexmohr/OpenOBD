#
# This is an example how scripting can be used.
# Here we simulate a vehicle driving
# The scripting is just executing commands and therefore does
# not support loops or variables.

# set_1 value
set_1 VehicleSpeed 0
set_1 EngineRPM 0

# sleep milliseconds
sleep 800

set_1 VehicleSpeed 20
set_1 EngineRPM 1000
sleep 800

set_1 VehicleSpeed 40
set_1 EngineRPM 2000
sleep 800

set_1 VehicleSpeed 50
set_1 EngineRPM 3000
sleep 800

set_1 VehicleSpeed 60
set_1 EngineRPM 4000
sleep 800

set_1 VehicleSpeed 70
set_1 EngineRPM 5000
sleep 800

set_1 VehicleSpeed 80
set_1 EngineRPM 6000
sleep 800

set_1 VehicleSpeed 90
set_1 EngineRPM 7000
sleep 800

set_1 VehicleSpeed 100
set_1 EngineRPM 8000
sleep 800

sleep 10000
set_1 VehicleSpeed 0
set_1 EngineRPM 0